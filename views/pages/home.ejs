<!-- it will list all schedules  -->

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home Page - Schedules Page</title>
</head>

<body>

    <%- include('../partials/head') %>
        <main>
            <table>
                <thead>
                    <tr>
                        <th colspan="1">User_Id</th>
                        <th colspan="1">Day</th>
                        <th colspan="1">Start_Time</th>
                        <th colspan="1">End_Time</th>
                    </tr>
                </thead>
                <tbody>
                    <% for ( i=0 ; i< schedules.length; i++){ %>
                        <tr>
                            <td>
                                <%= schedules[i].user_id %>
                            </td>
                            <td>
                                <%= schedules[i].day %>
                            </td>
                            <td>
                                <%= schedules[i].start_at %>
                            </td>
                            <td>
                                <%= schedules[i].end_at %>
                            </td>

                        </tr>
                        <% } %>
                </tbody>
            </table>
        </main>


</body>

</html>