<!-- it will list all schedules  -->
<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head') %>
</head>

<body>
<main>
    <table>
        <tr>
            <th>Employee</th>
            <th>Email</th>
        </tr>
        <% for (let i=0; i < users.length; i++) {%>
            <tr>
            <td><%=`${users[i].firstname} ${users[i].lastname}`%></td>
            <td><%=`${users[i].email}`%></td>
            </tr>
        <%}%>
    </table>
</main>
</body>

</html>