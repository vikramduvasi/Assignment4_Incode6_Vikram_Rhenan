<!-- it will list all schedules  -->
<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head') %>
</head>

<body>
    <div class="background">
        <header>
            <%- include('../partials/header') %>
        </header>
        <main class="w-100 d-flex flex-column align-items-center justify-content-center">
            <table class="w-75 shadow-lg rounded-borders bg-white">
                <tr class="p-3 text-center text-white bg-dark">
                    <th class="p-3 label-spacing round-top-left">FIRST NAME</th>
                    <th class="p-3 label-spacing">LAST NAME</th>
                    <th class="p-3 label-spacing round-top-right">EMAIL</th>
                </tr>
                <% for (let i=0; i < users.length; i++) {%>
                    <tr>
                        <td>
                            <%=users[i].firstname%>
                        </td>
                        <td>
                            <%=users[i].lastname%>
                        </td>
                        <td>
                            <%=users[i].email%>
                        </td>
                    </tr>
                    <%}%>
            </table>
        </main>
    </div>
</body>

</html>